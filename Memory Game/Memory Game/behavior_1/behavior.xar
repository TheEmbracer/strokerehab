        self.prevWin = True #keeps track of the outcome of the previous game

        self.maxTasks = 0 #keeps track of the max number of tasks per round

        self.numLives = 5 #init number of lives

        self.say = "" #Phrases Pepper will say

    def onLoad(self):
        #put initialization code here
        self.tts = ALProxy("ALTextToSpeech")
        self.logger.debug("Loaded box %s", self.getName())

    def onUnload(self):
        #put clean-up code here
        pass

    def onInput_onStart(self):

        self.calcNumTasks(self.prevWin)
        #INSERT TIMER HERE

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box

    def onInput_doneRound(self, win):
        if win:
            self.calcNumTasks(win)

        else:
            self.numLives-=1 #Decrement lives
            if self.numLives != 0: #if the player has not run out of lives, have the robot say 'Lets try again'
                self.say = "Let's try again."
                self.tts.say(self.say)
                self.calcNumTasks(win)

            else:
                self.say = "You have run out of lives. That's the end of the game. I hope you enjoyed playing, I know I did"
                self.tts.say(self.say)
                self.gameOver()
                self.onStopped()
            #end game, (ask if they want to see the results)
            self.onPrint("Num Lives: " + str(self.numLives))



    def calcNumTasks(self, win):
        #If they win two rounds in a row, increment max tasks
        if win and self.prevWin:
            self.maxTasks += 1

        #If they lose two rounds in a row, decrement max tasks
        elif not win and not self.prevWin:
            if self.maxTasks != 1:
                self.maxTasks -= 1

        #Randomize the number of tasks based on the max tasks
        if self.maxTasks > 2:
            #Randomizes with a range from max tasks -2 to max tasks
            self.numTasks = rand.randint(self.maxTasks-2, self.maxTasks)

        else:
            self.numTasks = rand.randint(1, 2)

        self.prevWin = win
        self.startNewRound(self.numTasks)

    def gameOver(self):
        self.onPrint("Game Over")
        self.onStopped()]]></content></script><Input name="onLoad" type="1" type_size="1" nature="0" inner="1" tooltip="Signal sent when diagram is loaded." id="1" /><Input name="onStart" type="1" type_size="1" nature="2" inner="0" tooltip="Box behavior starts when a signal is received on this input." id="2" /><Input name="onStop" type="1" type_size="1" nature="3" inner="0" tooltip="Box behavior stops when a signal is received on this input." id="3" /><Input name="doneRound" type="2" type_size="1" nature="1" inner="0" tooltip="0 = incorrect answer&#x0A;1 = correct answer" id="4" /><Input name="roundFinished" type="1" type_size="1" nature="1" inner="0" tooltip="" id="5" /><Output name="onStopped" type="1" type_size="1" nature="1" inner="0" tooltip="Signal sent when box behavior is finished." id="6" /><Output name="startNewRound" type="2" type_size="1" nature="2" inner="0" tooltip="Passes the number of tasks that should be held in the next round to the Round script." id="7" /><Output name="onSay" type="3" type_size="1" nature="2" inner="0" tooltip="" id="8" /><Output name="onPrint" type="3" type_size="1" nature="2" inner="0" tooltip="" id="9" /></Box><Box name="Print" id="5" localization="8" tooltip="" x="525" y="468"><bitmap>media/images/box/box-python-script.png</bitmap><script language="4"><content><![CDATA[class MyClass(GeneratedClass):
