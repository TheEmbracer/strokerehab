        for i in range(1, setsAndReps[0]+1):
            if i != 1:
                self.say = "Now we will take a break before the next set. When you are ready to begin, touch my hand."
                self.tts.say(self.say)
                while not self.newSet:
                    pass
                self.newSet = False
                self.say = "Set number " + str(i) + ", let's begin"
                self.tts.say(self.say)

            if i == 2:
                self.say = "Here is a tip: To help keep your shoulder from hiking up, place your stronger hand on it."
                self.tts.say(self.say)

            elif i == 3:
                self.say = "If you would like to make this exercise more difficult, try placing a weight around your weaker wrist or palm."
                self.tts.say(self.say)

            for j in range(1, setsAndReps[1]+1):
                self.say = str(j)
                self.startRep()
                while not self.motionComplete:
                    pass
                self.motionComplete=False
                self.tts.say(self.say)

        self.say = "You have completed the arm to front exercise [insert phrase from list]"
        self.tts.say(self.say)
