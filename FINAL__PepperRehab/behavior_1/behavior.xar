    def onInput_onStart(self):
        self.newSet = False
        self.motionComplete = False
        self.say = "We will do this stretch two times. Ready,\\pau=1000\\ begin!"
        self.tts.say(self.say)
        self.currSet = 1
        self.performExercise()

    def performExercise(self):
        self.startRep() #Bend down
        self.waitMvt() #Wait for movement to finish


        for j in range(1, 11): #Hold for 10 seconds
            self.say = str(j)
            self.tts.say(self.say)
            sleep(1.0)


        self.endRep() #Sit up
        self.waitMvt() #Wait for movement to finish



        if self.currSet == 1:
            self.say = "Now we will take a break. When you are ready to begin the second and final set, touch my hand."
            self.tts.say(self.say)
            while not self.newSet:
                pass
            self.newSet = False
            self.say = "Let's begin"
            self.tts.say(self.say)
            self.currSet += 1
            self.performExercise()

        else:
            self.say = "You have completed the arm to front exercise [insert phrase from list]"
            self.tts.say(self.say)
            self.onStopped()



    def waitMvt(self):
        while self.motionComplete == False:
                pass
        self.motionComplete = False

